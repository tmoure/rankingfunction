@InProceedings{10.1007/978-3-540-24622-0_20,
author="Podelski, Andreas
and Rybalchenko, Andrey",
editor="Steffen, Bernhard
and Levi, Giorgio",
title="A Complete Method for the Synthesis of Linear Ranking Functions",
booktitle="Verification, Model Checking, and Abstract Interpretation",
year="2004",
publisher="Springer Berlin Heidelberg",
address="Berlin, Heidelberg",
pages="239--251",
abstract="We present an automated method for proving the termination of an unnested program loop by synthesizing linear ranking functions. The method is complete. Namely, if a linear ranking function exists then it will be discovered by our method. The method relies on the fact that we can obtain the linear ranking functions of the program loop as the solutions of a system of linear inequalities that we derive from the program loop. The method is used as a subroutine in a method for proving termination and other liveness properties of more general programs via transition invariants; see [PR03].",
isbn="978-3-540-24622-0"
}
    
@InProceedings{10.1007/978-3-540-27813-9_6,
author="Tiwari, Ashish",
editor="Alur, Rajeev
and Peled, Doron A.",
title="Termination of Linear Programs",
booktitle="Computer Aided Verification",
year="2004",
publisher="Springer Berlin Heidelberg",
address="Berlin, Heidelberg",
pages="70--82",
abstract="We show that termination of a class of linear loop programs is decidable. Linear loop programs are discrete-time linear systems with a loop condition governing termination, that is, a while loop with linear assignments. We relate the termination of such a simple loop, on all initial values, to the eigenvectors corresponding to only the positive real eigenvalues of the matrix defining the loop assignments. This characterization of termination is reminiscent of the famous stability theorems in control theory that characterize stability in terms of eigenvalues.",
isbn="978-3-540-27813-9"
}

